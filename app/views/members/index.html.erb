<div id="static_box">
<span class="page_title">Meet the members</span><br>
<div class="search">
  <%= form_tag members_path, :class => "search_form", :method => :get do %>
    <div class="search-box" style="border: 1px solid red;"><%= text_field_tag :search_members, params[:search_members], :placeholder => "Someone Awesome!"%>
    <%= button_tag name="" %>
    </div>
  <% end %>
</div>

<table style="clear: both;">
  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td>
          <%= link_to member do %>
            <%= image_tag member.photo.url(:tiny_thumb), :class => "rounded_tiny_thumb"%>
            <span class="tiny_thumb_text"><%= member.user_name %></span>
            <span class="tiny_thumb_text"><%= member.occupation %></span></td>
          <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
